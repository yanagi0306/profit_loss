<div class="budgets-wrapper">
  <div class="main-content-title-wrap">
    閲覧・編集
    <div class="sub-buttons">
      <div id="add_up_down" class="d-inline">
        <a href="" class="info">CSVアップロード</a>
      </div>
    </div>
  </div>

  <div class="search">
    <div class="form-group">
      <label class="form-label">店舗</label>
      <div class="form-stores">
        <select name="search_store_code" class="input-item" id="search_store">
          <option>000:管理者用店舗（日本</option>
          <option>001:ＢＥＮＣＩＡ</option>
        </select>
      </div>
    </div>
    <div class="form-group">
      <label class="form-label">指定年月</label>
      <div class="form-inline">
        <select id="search_year" name="search_year" class="input-item text-end">
          <option>2020</option>
          <option selected="">2021</option>
          <option>2022</option>
        </select>
        <span>/</span>
        <select id="search_month" name="search_month" class="input-item">
          <option>1月</option>
          <option>2月</option>
          <option>3月</option>
          <option>4月</option>
          <option>5月</option>
          <option>6月</option>
          <option>7月</option>
          <option>8月</option>
          <option>9月</option>
          <option>10月</option>
          <option>11月</option>
          <option>12月</option>
        </select>
      </div>
    </div>
    <div class="form-group-btn">
      <input type="button" name="search" id="search" class="form__btn" value="検索"
        onclick="postdata(&#39;search&#39;);" /><!-- 検索 -->
    </div>
  </div>
  <div class="result-wrapper">
    <div class="result-title">
      予算入力情報
    </div>
  </div>
  <div class="result-content">
    <table class="budgets-table">
      <tr class='table-head'>
        <th class='search-column'>2020年11月</th>
        <th class='budgets-value'>予算値</th>
        <th class="last-month">前月実績</th>
      </tr>
      <tr>
        <td class='search-column'>売上</td>
        <td class='budgets-value'>
          <input type="text" class="input-group-append text-end">
        </td>
        <td class="last-month">20000</td>
      </tr>
      <tr>
        <td class='search-column'>ランチ売上</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>ディナー売上</td>
        <td class='budgets-value'><input type="text" class="form-controlinput-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>電気代</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>ガス代</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>水道代</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>P人件費</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>R人件費</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>食材費</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
      <tr>
        <td class='search-column'>その他</td>
        <td class='budgets-value'><input type="text" class="input-group-append text-end"></td>
        <td class="last-month">10000</td>
      </tr>
    </table>
  </div>
  <div class="form-group-btn">
    <input type="button" class="form__btn" value="登録">
  </div>
</div>
